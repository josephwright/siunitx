        {
          \siunitx_unit_format:nN {#3} \l_@@_tmp_tl
          \siunitx_print_unit:V \l_@@_tmp_tl
          \nobreak
        }
      \siunitx_quantity:nn {#2} {#4}
    \group_end:
  }
